pipeline{
    agent any
    stages{
        stage("source code checkout"){
            steps{
                echo"========source code checkout stage====="
                script{
                    try{
                        checkout scmGit(branches: [[name: '*/beanstalk-project']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/SivaSuribabu/elasticbeanstalk.git']])
                        echo"========source code checkout stage completed====="
                    } catch (err){
                        echo"========source code checkout stage failed====="
                        error"Checkout failed: ${err.message}"
                    }
                }
            }
        }
    }   
}